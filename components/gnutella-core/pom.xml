<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"> 
  <modelVersion>4.0.0</modelVersion>  
  <parent>
    <groupId>org.limewire</groupId>
    <artifactId>lw-components</artifactId>
    <version>1.0</version>
    <relativePath>../components/pom.xml</relativePath>
  </parent>

  <artifactId>lw-core</artifactId>
  <packaging>jar</packaging>
  <version>${limewire.version}</version>
  <name>LimeWire Core</name>

  <dependencies> 
	<!-- LimeWire Components -->
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-common</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-collection</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-io</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-nio</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-rudp</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-security</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-setting</artifactId>
      <version>${limewire.version}</version>
    </dependency> 
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-statistic</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-lib</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>mojito</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-http</artifactId>
      <version>${limewire.version}</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>lw-lwsserver</artifactId>
      <version>${limewire.version}</version>
    </dependency>
	<!-- Customized LimeWire Jars --> 
	<dependency>
      <groupId>org.limewire</groupId>
      <artifactId>clink</artifactId>
      <version>0.6-lw-1</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>commons-httpclient</artifactId>
      <version>2.0-rc2-lw-1</version>
    </dependency>
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>id3v2</artifactId>
      <version>0.3-lw-1</version>
    </dependency>
	<!-- Other Non-Ibiblio Jars -->
    <dependency>
      <groupId>com.jcraft</groupId>
      <artifactId>jogg</artifactId>
      <version>0.0.7</version>
    </dependency>
    <dependency>
      <groupId>com.jcraft</groupId>
      <artifactId>jorbis</artifactId>
      <version>0.0.15</version>
    </dependency>
    <dependency>
      <groupId>com.onionnetworks.fec</groupId>
      <artifactId>onion-common</artifactId>
      <version>1.0</version>
    </dependency>
    <dependency>
      <groupId>com.onionnetworks.fec</groupId>
      <artifactId>onion-fec</artifactId>
      <version>1.0</version>
    </dependency>
	<!-- Ibiblio Jars -->
    <dependency>
      <groupId>commons-net</groupId>
      <artifactId>commons-net</artifactId>
      <version>1.4.1</version>
    </dependency>
<!--     <dependency> -->
<!--       <groupId>com.jcraft</groupId> -->
<!--       <artifactId>jsch</artifactId> -->
<!--       <version>0.1.23</version> -->
<!--     </dependency> -->
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.14</version>
    </dependency>	
    <dependency>
      <groupId>org.jaudiotagger</groupId>
      <artifactId>jaudiotagger</artifactId>
      <version>1.0</version>
    </dependency>	
	<!-- Scoped Jars -->
    <dependency>
      <groupId>org.limewire</groupId>
      <artifactId>cocoaFoundation</artifactId>
      <version>1</version>
	  <scope>compile</scope>
    </dependency>
  </dependencies>

  <build>
    <sourceDirectory>${basedir}</sourceDirectory>
	<plugins>
	  <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <excludes>
            <exclude>examples/</exclude>
          </excludes>
        </configuration>
      </plugin>
	  <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <excludes>
            <exclude>examples/</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>

</project>
