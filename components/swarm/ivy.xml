<ivy-module version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info organisation="org.limewire" module="swarm"/>
    <configurations>
        <include file="../common/common-configurations.xml"/>
    </configurations>
    <publications>
        <artifact name="swarm" conf="master"/>
    </publications>
    <dependencies>
        <dependency org="org.limewire" name="common" rev="latest.integration" conf="compile,runtime"/>
        <dependency org="org.limewire" name="collection" rev="latest.integration" conf="compile,runtime"/>
        <dependency org="org.limewire" name="io" rev="latest.integration" conf="compile,runtime"/>
        <dependency org="org.limewire" name="nio" rev="latest.integration" conf="compile,runtime"/>
        <dependency org="org.limewire" name="http" rev="latest.integration" conf="compile,runtime"/>
        <dependency org="org.apache.httpcomponents" name="httpcore" rev="${httpcore.version}" conf="compile,runtime"/>
        <dependency org="org.apache.httpcomponents" name="httpcore-nio" rev="${httpcore-nio.version}" conf="compile,runtime"/>
        <dependency org="commons-logging" name="commons-logging" rev="${commons-logging.version}" conf="compile,runtime"/>
        <dependency org="com.google.code.guice" name="guice" rev="${guice.version}" conf="compile,runtime"/>
        <dependency org="org.limewire" name="test-util" rev="latest.integration" conf="test"/>
    </dependencies>
</ivy-module>