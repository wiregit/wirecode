<ivy-module version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info organisation="org.limewire" module="test-util"/>
    <configurations>
        <include file="../common/ivy-configs.xml"/>
    </configurations>
    <publications>
        <artifact name="test-util" conf="master"/>
    </publications>
    <dependencies>
        <dependency org="none" name="guice" rev="${guice.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/junit" rev="${junit.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/cglib-nodep" rev="${cglib-nodep.version}" force="true" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/hamcrest-library" rev="${hamcrest-library.version}" force="true" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/hamcrest-core" rev="${hamcrest-core.version}" force="true" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/jmock" rev="${jmock.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/jmock-junit3" rev="${jmock-junit3.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/jmock-junit4" rev="${jmock-junit4.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/jmock-legacy" rev="${jmock-legacy.version}" conf="compile,runtime"/>
        <dependency org="none" name="tests/jmock/objenesis" rev="${objensis.version}" conf="compile,runtime"/>
        <dependency org="none" name="commons-logging" rev="${commons-logging.version}" conf="compile,runtime"/>
        <dependency org="none" name="log4j" rev="${log4j.version}" conf="runtime"/>
    </dependencies>
</ivy-module>
